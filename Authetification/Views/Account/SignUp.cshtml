@model SignUpViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>Sign Up</h4>

<form method="post">
    <div>
        <label asp-for="Email"></label>
        <input asp-for="Email" />
        <span asp-validation-for="Email"></span>
    </div>
    <div>
        <label asp-for="Pswd"></label>
        <input asp-for="Pswd" />
        <span asp-validation-for="Pswd"></span>
    </div>
    <div>
        <label asp-for="Pswd2"></label>
        <input asp-for="Pswd2" />
        <span asp-validation-for="Pswd2"></span>
    </div>
    <div>
        <input type="submit" value="SignUp" />
    </div>
</form>

